{
  "name": "serverless-aws-lambda",
  "version": "1.3.8",
  "description": "AWS Application Load Balancer and API Gateway - Lambda dev tool for Serverless. Allows Express synthax in handlers. Supports packaging, local invoking and offline real ALB and APG lambda server mocking",
  "main": "dist/index.js",
  "author": "Inqnuam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inqnuam/serverless-aws-lambda.git"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "esbuild": "^0.15.9",
    "esbuild-node-externals": "^1.5.0",
    "serve-static": "^1.15.0",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/node": "^18.7.14",
    "@types/serve-static": "^1.15.0"
  },
  "scripts": {
    "dev": "tsc --p ./src/lambda/tsconfig.json && rm -rf ./express && mv -f ./dist/lambda/* . && DEV=true node ./build.js ",
    "build": "node ./build.js && tsc --p ./src/lambda/tsconfig.json && rm -rf ./express && mv -f ./dist/lambda/* ./"
  }
}
