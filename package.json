{
  "name": "serverless-aws-lambda",
  "version": "4.0.2",
  "description": "AWS Application Load Balancer and API Gateway - Lambda dev tool for Serverless. Allows Express synthax in handlers. Supports packaging, local invoking and offline real ALB and APG lambda server mocking.",
  "main": "./dist/index.js",
  "typings": "./dist/config.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js"
    },
    "./router": {
      "types": "./dist/lambda/router.d.ts",
      "require": "./dist/lambda/router.cjs",
      "import": "./dist/lambda/router.mjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.cjs",
      "import": "./dist/server.mjs"
    },
    "./body-parser": {
      "require": "./plugins/body-parser.js"
    }
  },
  "author": "Inqnuam",
  "license": "MIT",
  "homepage": "https://github.com/inqnuam/serverless-aws-lambda",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inqnuam/serverless-aws-lambda.git"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "esbuild": "^0.16.17",
    "esbuild-node-externals": "^1.6.0",
    "serve-static": "^1.15.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/node": "^14.14.31",
    "@types/serve-static": "^1.15.0"
  },
  "scripts": {
    "dev": "DEV=true node ./build.mjs ",
    "build": "node ./build.mjs"
  }
}
